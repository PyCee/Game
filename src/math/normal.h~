
#ifndef _NORMAL_
#define _NORMAL_

typedef struct _Matrix_t Normal_t;

void genNormal2(Normal_t **);
void genNormal3(Normal_t **);

void freeNormal2(Normal_t **);
void freeNormal3(Normal_t **);

void NormalizeNormal2(Normal_t *);
void NormalizeNormal3(Normal_t *);

void PrintNormal2(Normal_t *);
void PrintNormal3(Normal_t *);
#endif
